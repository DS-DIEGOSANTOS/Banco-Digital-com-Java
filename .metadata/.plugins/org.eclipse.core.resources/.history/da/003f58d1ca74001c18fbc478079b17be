package Banco;

public class Cliente {
	
	String nome;
	int conta;
	double saldo = 0;
	
	public Cliente(String nome,int conta) {
		this.nome = nome;
		this.conta = conta;
	}
	
	void informações() {
		System.out.println("nome:"+nome);
		System.out.println("conta:"+conta);
		System.out.println("saldo:"+saldo);
	}

}
